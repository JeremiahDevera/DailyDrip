<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DailyDrip">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DailyDrip"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project">
        <activity
            android:name="com.example.DailyDrip.Loginlogout.Register"
            android:exported="false" />
        <activity
            android:name="com.example.DailyDrip.Loginlogout.Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.DailyDrip.Activity.CartListActivity" />
        <activity android:name="com.example.DailyDrip.Activity.ShowDetailActivity" />
        <activity android:name="com.example.DailyDrip.Activity.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.DailyDrip.Activity.MainActivity" />
    </application>

</manifest>